html, body {
  width: 100%;
  height: 100%;
}

img {
  display: block;
}

ul li {
  list-style: none;
}

html, body, ul, li, img, a {
  margin: 0;
  padding: 0;
}

a {
  text-decoration: none;
}

i {
  font-style: normal;
}

.wrapper {
  width: 100%;
  height: auto;
}

.wrapper nav {
  height: 1.28rem;
  width: 100%;
  background: #F9F9F9;
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 100;
  border-top: solid .02rem #E1E1E1;
}
.wrapper nav ul li {
  float: left;
  width: 2.46rem;
  height: 1.30rem;
}
.wrapper nav ul li a {
  display: block;
}
.wrapper nav ul li a i {
  display: block;
  width: 100%;
  height: .8rem;
  text-align: center;
  color: #757575;
  padding-top: .1rem;
  box-sizing: border-box;
}
.wrapper nav ul li a .icon1 {
  padding-top: .2rem;
  font-size: .4rem;
}
.wrapper nav ul li a .icon2 {
  font-size: .6rem;
}
.wrapper nav ul li a .icon3 {
  font-size: .6rem;
}
.wrapper nav ul li a .icon4 {
  padding-top: .2rem;
  font-size: .5rem;
}
.wrapper nav ul li a .icon5 {
  font-size: .6rem;
}
.wrapper nav ul li a span {
  display: block;
  width: 100%;
  text-align: center;
  color: #666666;
  height: .4rem;
  line-height: .3rem;
  font-size: .28rem;
}


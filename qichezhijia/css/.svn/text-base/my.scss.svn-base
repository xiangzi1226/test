.my{
    width: 12.42rem;
    height: 22rem;
    position: relative;
    overflow: hidden;
        .container{
//      display: none;
        position: absolute;
        left: 0;
        top: 0;
        z-index: 10;
        height: auto;;
        width: 100%;
        .header{
        width: 100%;
        height: 1.2rem;
        i{
            display: block;
            width: 100%;
            height: 1.2rem;
            line-height: 1.2rem;
            box-sizing: border-box;
            text-align: right;
            padding-right: .54rem;
            color: #555555;
            font-size: .62rem;
        }
    }
    .account{
      height: 7.5rem;
      width: 100%;
      .logo{
        height: 2.28rem;
        width: 2.28rem;
        border: solid 1px #333;
        border-radius: 100%;
        margin-left: 5.06rem;
        margin-top: .26rem;
        overflow: hidden;
        img{
          height: 100%;
          width: 100%;
        }
      }
      .name{
        height: 1.46rem;
        width: 2.28rem;
        text-align: center;
        margin-left: 5.06rem;
        line-height: 1.46rem;
        font-size: .44rem;
        color: #4F4F4F;
      }
      .minlogo{
        height: 3.5rem;
        width: 100%;
        ul{
          width: 100%;
          height: 3.5rem;
          li{
              width: 4.12rem;
            float: left;
            i{
              margin-top: 1.5rem;
              margin-bottom: .24rem;
              display: block;
              text-align: center;
              color: #787878;
              font-size: .64rem;
            }
            span{
              display: block;
              text-align: center;
              color: #8C8C8C;
              width: 4.12rem;
              font-size: .34rem;
            }
          }
        }
      }
    }
    .blank{
      width: 100%;
      height: .28rem;
      background: #F0EFF5;
    }
    .collect{
      height: 1.52rem;
      width: 100%;
      box-sizing: border-box;
      padding: 0 .52rem 0 .46rem;
      .left{
        width:10.5rem;
        height: 1.52rem;
        line-height: 1.52rem;
        float: left;
        box-sizing: border-box;
        i{
          font-size: .54rem;
          color: #FFCC00;
          float: left;
           margin-right:.46rem;
        }
        span{
            font-size: .44rem;
            color: #333333;
            float: left;
        }
      }
      .right{
        font-size: .36rem;
        color: #3333333;
        float: left;
        line-height:  1.52rem;
      }
    }
    .privilege{
      height: 1.52rem;
      width: 100%;
      box-sizing: border-box;
      padding: 0 .52rem 0 .46rem;
      .left{
        width:10.5rem;
        height: 1.52rem;
        float: left;
        line-height: 1.52rem;
        box-sizing: border-box;
        i{
          font-size: .54rem;
          color: #F85858;
          float: left;
           margin-right:.46rem;
        }
        span{
            font-size: .44rem;
            color: #333333;
             float: left;
        }
      }
      .right{
        font-size: .36rem;
        color: #333333;
        float: left;
        line-height:  1.52rem;
      }
    }
    .history{
        box-sizing: border-box;
        width: 100%;
        height: 3rem;
         padding-left: .46rem;
        .history-top{
            height: 1.48rem;
            width: 100%;
            .icon1{
                display: block;
                float: left;
                width: .74rem;
                height: 1.48rem;
                line-height: 1.48rem;
                font-size: .52rem;
                color: #333333;
            }
            .border-btm{
                float: left;
                 height: 1.48rem;
                line-height: 1.48rem;
                width: 11.22rem;
                box-sizing: border-box;
                border-bottom: solid .01rem #E9E9E9;
                padding-left: .28rem;
                span{
                    float: left;
                    font-size: .4rem;
                    color: #333333;
                    display: block;
                    width: 9.6rem;
                }
                .icon2{
                    display: block;
                    float: left;
                    width: 1.1rem;
                    height: 1.48rem;
                    line-height: 1.48rem;
                    font-size: .52rem;
                    color: #333333;
                }
            }
        }
        .history-bottom{
            height: 1.48rem;
            width: 100%;
            .icon1{
                display: block;
                float: left;
                width: .74rem;
                height: 1.48rem;
                line-height: 1.48rem;
                font-size: .52rem;
                color: #333333;
            }
            .border-btm{
                float: left;
                 height: 1.48rem;
                line-height: 1.48rem;
                width: 11.22rem;
                box-sizing: border-box;
                padding-left: .28rem;
                span{
                    float: left;
                    font-size: .4rem;
                   color: #333333;
                    display: block;
                    width: 9.6rem;
                }
                .icon2{
                    display: block;
                    float: left;
                    width: 1.1rem;
                    height: 1.48rem;
                    line-height: 1.48rem;
                    font-size: .52rem;
                    color: #555555;
                }
            }
        }
    }
    .setting{
      height: 1.52rem;
      width: 100%;
      box-sizing: border-box;
      padding: 0 .52rem 0 .46rem;
      .left{
        width:10.5rem;
        height: 1.52rem;
        line-height: 1.52rem;
        float: left;
        box-sizing: border-box;
        i{
          font-size: .54rem;
          color: #555555;
           float: left;
           margin-right:.46rem;
        }
        span{
            font-size: .44rem;
            color: #333333;
             float: left;
           
        }
      }
      .right{
        font-size: .36rem;
        color: #3333333;
        float: left;
        line-height:  1.52rem;
      }
    }
    .color{
        height: 3.2rem;
        width: 100%;
        background: #F0EFF5;
    }
    }
    .login{
        background: #fff;
//      display: none;
        position: absolute;
        left: 12.42rem;
        top: 0;
        z-index: 20;
        height: 100%;
        width: 100%;
        .login-banner{
            height: 6.6rem;
            width: 100%;
            img{
                height: 100%;
                width: 100%;
            }          
        }
        .exit{
            position: absolute;
            left: 0;
            top: 0;
            height: 1.3rem;
            width: 1.44rem;
            z-index: 100;
        }
        .form{
            margin-top: .55rem;
            height: 7.44rem;
            width: 100%;
            box-sizing: border-box;
            padding: 0 .9rem 0 .9rem;
            input{
                outline: none;
                border: 0;
            }
            .username{
                height: 1.8rem;
                width: 100%;
                border-bottom: solid 1px #E9E9E9;
                font-size: .46rem;
                color: #CCCCCC;
                line-height: 2.2rem;
            }
            .pwd{
                height: 1.78rem;
                width: 100%;
                border-bottom: solid 1px #E9E9E9;
                font-size: .46rem;
                color: #CCCCCC;
                line-height: 2.2rem;
            }
            .btn{
                outline: none;
                border: 0;
                height: 1.36rem;
                width: 100%;
                background: #8DB4FF;
                color: #D1E1FF;
                margin-top: 1.2rem;
                font-size: .42rem;
                border-radius: .16rem;
            }
            .login-text{
                height: 1.28;
                width:100%;
                .sp1{
                    font-size: .36rem;
                    display: block;
                    float: left;
                    color: #999999;
                    width: 1rem;
                    height: 1.28;
                    line-height: 1.28rem;
                }
                .sp2{
                    font-size: .36rem;
                    display: block;
                    float: right;
                    color: #999999;
                    width: 2rem;
                    height: 1.28;
                    line-height: 1.28rem;
                }
            }
        }
    }
    .regest{
        background: #fff;
        position: absolute;
        left: 12.42rem;
        top: 0;
        z-index: 30;
        height: 100%;
        width: 100%;
        #head{
            height: 1.74rem;
            line-height: 1.74rem;
            width: 100%;
            border-bottom: 1px solid #E0E0E0;
            text-align: center;
            position: relative;
            font-size: 0.48rem;
            color: #161616;
            #zjt{
                position: absolute;
                left: 0.44rem;
                font-size: 0.64rem;
                height: 1.74rem;
                line-height: 1.74rem;
                top: 0;
            }
            #zjt::before{
                content: '\e615';
            }
        }
        .form{
            margin-top: .55rem;
            height: 7.44rem;
            width: 100%;
            box-sizing: border-box;
            padding: 0 .9rem 0 .9rem;
            input{
                outline: none;
                border: 0;
            }
            .name{
                position: relative;
                .username{
                height: 1.8rem;
                width: 100%;
                border-bottom: solid 1px #E9E9E9;
                font-size: .46rem;
                color: #CCCCCC;
                line-height: 2.2rem;
              }
              .phone-error{
                  position: absolute;
                  right: 0;
                  top: 0.85rem;
                  font-size: 0.13rem;
                  color:red;
                  display: none;
              }
            }
            .password{
                position: relative;
                .pwd{
                height: 1.78rem;
                width: 100%;
                border-bottom: solid 1px #E9E9E9;
                font-size: .46rem;
                color: #CCCCCC;
                line-height: 2.2rem;
               }
               .pwd-error{
                  position: absolute;
                  right: 0;
                  top: 0.85rem;
                  font-size: 0.13rem;
                  color:red;
                  display: none;
               }
            }
            
            .btn{
                font-size: .42rem;
                outline: none;
                border: 0;
                height: 1.36rem;
                width: 100%;
                background: #8DB4FF;
                color: #D1E1FF;
                margin-top: 1.2rem;
                border-radius: .16rem;
            }
            .regest-text{
                height: 1.28;
                width:100%;
                font-size: .36rem;
                line-height: 1.28rem;
                .sp1{
                    color: #9B9B9B;
                }
                .sp2{
                    color: #367DFF;
                }
            }
        }
    }

}
